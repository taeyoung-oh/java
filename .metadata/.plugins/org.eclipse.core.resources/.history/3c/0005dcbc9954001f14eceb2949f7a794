package Test01;

public class Test999 {
	public static void main(String[] args) {
		boolean answer = true;
		String s = ")()(";
        if(s.length() % 2 == 1) answer = false;
        else {
            if(s.charAt(0) == ')' || s.charAt(s.length()-1) == '(') answer = false;
        }
        
        System.out.println(answer);
	}
}