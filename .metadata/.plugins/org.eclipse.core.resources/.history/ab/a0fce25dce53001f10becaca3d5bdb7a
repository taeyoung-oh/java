package Test01;

public class Test999 {
	public static void main(String[] args) {
		int n = 12345;
		int temp = n;
		int temp2 = n;
		int size = 0;
		
		while(temp >= 1) {
            temp /= 10;
            size++;
        }
		int[] answer = new int[size];
        for(int i = 0; i < size; i++) {
            answer[0] = temp2 % 10;
            temp2 /= 10;
            System.out.println(answer[i]);
        }
        
	}
	
}