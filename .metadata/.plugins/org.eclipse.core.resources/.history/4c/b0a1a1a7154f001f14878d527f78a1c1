package Test01;

import java.util.Scanner;

public class Test999 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int arr1[] = {1, 2, 3, 4};
		int arr2[] = arr1;
		arr2[0] = 5;
		
		for(int i = 0; i < arr2.length; i++) {
			System.out.println(arr2[i]);
		}
		
	}
}