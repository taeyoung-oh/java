package Test01;

public class Test999 {
	public static void main(String[] args) {
		int x = 2;
		int n = 5;
		long[] answer = {};
        for(int i = 1; i < n+1; i++){
            answer[i] = i * x;
        }
        for(int i = 1; i < n+1; i++){
            System.out.println(answer[i]);
        }
	}
	
}